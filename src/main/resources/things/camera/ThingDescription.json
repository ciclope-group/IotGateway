{
  "type": "Thing",
  "name": "camera",
  "base": "http://wotgate.om.fi.upm.es/things/camera/",
  "gatekeeper": "http://wotgate.om.fi.upm.es/things/gatekeeper/",
  "roleBasedWritingAccessControl": "Administrator",
  "interactions": [
    {
      "type": "Property",
      "name": "state",
      "outputData": {
        "type": "object",
        "fields": {
          "name": {
            "type": "string"
          },
          "operatingStatus": {
            "enum": [
              "OK",
              "WORKING",
              "ERROR",
              "UNKNOWN"
            ]
          },
          "numberOfStoredCaptures": {
            "type": "integer"
          },
          "timestamp": {
            "type": "string"
          }
        }
      },
      "writable": false,
      "link": [
        {
          "href": "state",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "type": "Action",
      "name": "captureImage",
      "roleBasedAccessControl": "Authenticated",
      "reservable": "true",
      "inputData": {
        "type": "object",
        "fields": {
          "exposureTime": {
            "type": "integer"
          },
          "brightness": {
            "type": "integer"
          },
          "gain": {
            "type": "integer"
          },
          "quality": {
            "type": "integer"
          }
        },
        "required": [
          "exposureTime",
          "brightness",
          "gain",
          "quality"
        ]
      },
      "outputData": {
        "type": "object",
        "fields": {
          "id": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      },
      "link": [
        {
          "href": "captureImage",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "type": "Action",
      "name": "cancelCaptureImage",
      "roleBasedAccessControl": "Authenticated",
      "reservable": "true",
      "link": [
        {
          "href": "cancelCaptureImage",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "type": "Action",
      "name": "getAllStoredCaptures",
      "asynchronous": "false",
      "outputData": {
        "type": "array",
        "items": {
          "type": "object",
          "fields": {
            "id": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "href": {
              "type": "string"
            },
            "exposureTime": {
              "type": "integer"
            },
            "brightness": {
              "type": "integer"
            },
            "gain": {
              "type": "integer"
            },
            "quality": {
              "type": "integer"
            },
            "userName": {
              "type": "string"
            },
            "dateCreated": {
              "type": "string"
            },
            "dateModified": {
              "type": "string"
            }
          }
        },
        "minItems": 0
      },
      "link": [
        {
          "href": "getAllStoredCaptures",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "type": "Action",
      "name": "getStoredCapture",
      "asynchronous": "false",
      "inputData": {
        "type": "object",
        "fields": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "outputData": {
        "type": "object",
        "fields": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "href": {
            "type": "string"
          },
          "exposureTime": {
            "type": "integer"
          },
          "brightness": {
            "type": "integer"
          },
          "gain": {
            "type": "integer"
          },
          "quality": {
            "type": "integer"
          },
          "userName": {
            "type": "string"
          },
          "dateCreated": {
            "type": "string"
          },
          "dateModified": {
            "type": "string"
          }
        }
      },
      "link": [
        {
          "href": "getStoredCapture",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "type": "Action",
      "name": "updateStoredCaptureInformation",
      "roleBasedAccessControl": "Authenticated",
      "asynchronous": "false",
      "inputData": {
        "type": "object",
        "fields": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "description"
        ]
      },
      "link": [
        {
          "href": "updateStoredCaptureInformation",
          "mediaType": "application/json"
        }
      ]
    }
  ]
}
